struct Test {
	ui8 val1;
	ui8 val2;
	ui16 val3;
	ui32 val4;
	ui64 val5;
}

ui64 Sum(ref<Test> test) {
	ui64 sum1 = (test.val1) + (test.val2);
	ui64 sum2 = (test.val3) + (test.val4);
	ui64 sum3 = sum1 + sum2;
	ui64 sum4 = sum3 + (test.val5);
	return sum4;
}

i32 main(string[] argv) {
	Test t = {0, 1, 2, 3, 4};
	Test t2 = {
		.val2 = 1;
		.val1 = 0;
		.val4 = 3;
		.val3 = 2;
		.val5 = 4;
	};
	t2.val1 = 5;
	ui64 summed = Sum(\t2);
	return summed;
}
