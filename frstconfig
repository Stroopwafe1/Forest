[Conventions]:
#public:
functions=$symbolName
constants_case=c_$SYMBOL_NAME

#private:
functions=__$symbolName

[Scripts]:
build=forest build .
install=forest plant <tree name>(package)
uninstall=forest chop <tree name>
force-remove=forest harvest <tree name>
update=forest water <tree name>
update-all=forest rain .
publish=forest ship .
